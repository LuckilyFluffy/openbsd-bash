# This file is .profile, which contains environment settings for all shells.

# Set the search path for executables.
PATH=$HOME/bin:/bin:/sbin:/usr/bin:/usr/sbin:/usr/X11R6/bin:/usr/local/bin:/usr/local/sbin:/usr/games:.

# Export the PATH variable so that child processes inherit the search path.
export PATH 

# Set the HOME variable to the user's home directory.
export HOME

# Set the TERM variable to the user's terminal type.
export TERM
